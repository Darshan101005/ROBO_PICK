{
    "hosting": {
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    },
    "emulators": {
      "auth": {
        "port": 9099
      },
      "functions": {
        "port": 5001
      },
      "firestore": {
        "port": 8080
      },
      "storage": {
        "port": 9199
      },
      "ui": {
        "port": 3000,
        "command": "npm run dev",
        "name": "Next.js Dev Server"
      }
    }
  }